# Local development without Docker
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# Use SQLite for local development
DATABASE_URL=sqlite+aiosqlite:///./local_dev.db
SYNC_DATABASE_URL=sqlite:///./local_dev.db

# Qdrant - will be mocked/skipped for local dev
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=medical_documents

# Application Configuration
APP_NAME=Medical Notes Processor
APP_VERSION=0.1.0
DEBUG=True
LOG_LEVEL=INFO

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# External APIs
NLM_API_BASE_URL=https://rxnav.nlm.nih.gov/REST
CLINICALTABLES_API_BASE_URL=https://clinicaltables.nlm.nih.gov/api
